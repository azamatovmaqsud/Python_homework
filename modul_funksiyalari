def avto_info(kompaniya, model, rangi, korobka, yili, narhi=None):
    """Avtomobil haqidagi ma'lumotlarni lug'at ko'rinishida qaytaruvchi funksiya"""
    avto = {'kompaniya':kompaniya,
            'model':model,
            'rang':rangi,
            'korobka':korobka,
            'yil':yili,
            'narh':narhi}
    return avto

def info_print(avto_info):
    """Avtomobillar haqida ma'lumotlar saqlangan lug'atni konsolga chiqaruvchi funksiya"""
    print(f"{avto_info['rang'].title()} {avto_info['kompaniya'].upper()} "
          f"{avto_info['model'].upper()}, {avto_info['korobka']} korobka, "
          f"{avto_info['yil']}-yil, {avto_info['narh']}$")

def user_data(first_name, last_name,ago):
    """familyasi ismi yoshini sorovchi funkisya"""
    malumot=[]
    print(f"Ismi: {first_name}")
    print(f"familyasi: {last_name}")
    print(f"yoshi: {ago}")
ismi=input("ismingizni kiriting: ")
familyasi=input('familyasi: ')
yoshi=input('yosh: ')
user_data(familyasi,ismi,yoshi)
